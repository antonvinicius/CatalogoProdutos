<MudMessageBox @ref="mbox" Title="@Title" CancelText="@null" YesText="@null">
    <MessageContent>
        @ChildContent
    </MessageContent>
</MudMessageBox>
@code {
    [Parameter]
    public EventCallback<bool?> Submit { get; set; }

    [Parameter]
    public RenderFragment ChildContent { get; set; }

    [Parameter]
    public string Title { get; set; }

    [Parameter]
    public string Icone { get; set; }

    [Parameter]
    public MudMessageBox mbox { get; set; }

    bool? result;
}
